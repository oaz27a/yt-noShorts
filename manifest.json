{
    "manifest_version": 3,
    "name": "YouTube NoShorts",
    "version": "1.1",
    "description": "Remove YouTube Shorts for better experience",
    "icons": {
      "128": "icon.png"
    },
    "content_scripts": [
      {
        "matches": ["*://*.youtube.com/*"],
        "js": ["content.js"],
        "run_at": "document_end"
      }
    ],
    "permissions": ["storage"]
  }